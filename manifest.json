{
  "name": "Jisho Lookup Tracker",
  "version": "0.1",
  "description": "Track your lookup history for jisho.org",
  "content_scripts": [
    {
      "matches": ["*://jisho.org/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["activeTab"],
  "web_accessible_resources": ["style.css"],
  "manifest_version": 2
}
